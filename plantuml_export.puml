@startumlclass Seller {    - sellerId: int    - name: string    - email: string    - rating: double    + createAuction()    + manageAuction()}class Buyer {    - buyerId: int    - name: string    - email: string    - rating: double    + placeBid()    + makePayment()    + writeReview()}class Auction {    - auctionId: int    - sellerId: int   ' FK -> Seller    - title: string    - status: string    + startAuction()    + closeAuction()    + checkStatus()}class Item {    - itemId: int    - auctionId: int  ' FK -> Auction    - sellerId: int   ' FK -> Seller    - title: string    + showDetails()}class Category {    - categoryId: int    - name: string    - description: string    - parentCategoryId: int  ' FK -> Category    + showCategory()}class Bid {    - bidId: int    - buyerId: int   ' FK -> Buyer    - itemId: int    ' FK -> Item    - amount: double    + placeBid()}class Payment {    - paymentId: int    - buyerId: int   ' FK -> Buyer    - itemId: int    ' FK -> Item    - amount: double    + processPayment()}class Review {    - reviewId: int    - itemId: int    ' FK -> Item    - buyerId: int   ' FK -> Buyer    - rating: int    + submitReview()}Seller "1" *--> "N" AuctionAuction "1" *--> "N" ItemCategory "1" *--> "N" ItemBuyer "1" *-up-> "N" BidBuyer "1" *-up-> "N" PaymentItem "1" *--> "N" BidItem "1" *--> "N" PaymentBuyer "1" *-up-> "N" ReviewItem "1" *--> "N" Review@enduml